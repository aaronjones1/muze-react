{
  "name": "muze",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@auth0/auth0-react": "^2.0.0",
    "@headlessui/react": "^1.7.7",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "npx tailwindcss -i ./src/index.css -o ./public/tailwind.css",
    "build:storybook": "storybook build",
    "build-storybook": "concurrently \"yarn:build:*\"",
    "watch:css": "npx tailwindcss -i ./src/index.css -o ./public/tailwind.css --watch",
    "watch:storybook": "storybook dev -p 6006 -s public",
    "storybook": "concurrently \"yarn:watch:*\"",
    "//": "build-storybook\": \"build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.0.0-beta.36",
    "@storybook/addon-essentials": "^7.0.0-beta.36",
    "@storybook/addon-interactions": "^7.0.0-beta.36",
    "@storybook/addon-links": "^7.0.0-beta.36",
    "@storybook/node-logger": "^7.0.0-beta.36",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^7.0.0-beta.36",
    "@storybook/react-webpack5": "7.0.0-beta.36",
    "@storybook/testing-library": "^0.0.14-next.1",
    "babel-plugin-named-exports-order": "^0.0.2",
    "concurrently": "^7.6.0",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.0-beta.36",
    "tailwindcss": "^3.2.4",
    "webpack": "^5.75.0"
  }
}
